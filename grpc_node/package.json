{
  "name": "grpc_node",
  "version": "1.0.0",
  "description": "grpc node test poc",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "nodemon --watch server.ts --exec ts-node server.ts",
    "dev:client": "nodemon --watch client.ts --exec ts-node client.ts",
    "build:proto": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./ ./proto/*.proto --ts_proto_opt=outputServices=grpc-js,env=node,esModuleInterop=true"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "protoc": "^1.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.6",
    "ts-proto": "^1.173.0"
  }
}
